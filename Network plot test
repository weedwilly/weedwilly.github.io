library(netmeta)

# 假設有三種藥物 A, B, C 的比較試驗
treatments <- c("A", "B", "C")
studies <- data.frame(
  study = c("Trial1", "Trial2", "Trial3"),
  treat1 = c("A", "A", "B"),
  treat2 = c("B", "C", "C"),
  TE = c(-0.2, -0.1, 0.3),  # 效果值
  seTE = c(0.1, 0.15, 0.2)  # 標準誤
)

net <- netmeta(TE, seTE, treat1, treat2, studlab = study, data = studies, sm = "OR")
netgraph(net, thickness = "number.of.studies", plastic = TRUE)
